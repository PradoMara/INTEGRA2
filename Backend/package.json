{
  "name": "marketplace-api",
  "version": "1.0.0",
  "description": "API Backend for Marketplace App",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon --ignore uploads/ --ignore public/ server.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "node prisma/seed.js",
    "db:seed:categories": "node prisma/seed_categories.js",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "verify": "node verify-setup.js",
    "setup": "node setup-postgresql.js",
    "security:generate-secrets": "node scripts/generate-jwt-secrets.js",
    "security:audit": "npm audit && npm audit fix"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "express-validator": "^7.0.1",
    "firebase-admin": "^13.6.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "socket.io": "^4.7.5"
  },
  "devDependencies": {
    "@prisma/client": "^6.16.3",
    "nodemon": "^3.0.1",
    "prisma": "^6.16.3"
  }
}
